				$('.nav-link[href="'+hash+'"]').trigger('click');
				$('html,body').animate({ scrollTop: 0 }, 0);
			}

			$('.design-book-wrapper .item').hover(function() {

			}, function() {
				if($(this).find('.dropdown-menu').hasClass('show')) {
					$(this).find('.dropdown-menu.show').removeClass('show');
				}
			});
		});
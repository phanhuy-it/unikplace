extends common/layout.pug
block custom
	-
		let pageName = 'Your Place'
		let pageFileName = path.basename(`${website + string2slug(pageName)}.html`)

block meta-title
	title #{pageName} | #{projectName}

block content
	.container.p-t-7.p-t-sm-9
		.m-b-5
			nav.breadcrumb-wrapper.breadcrumb-inline.justify-content-center.mb-0
				.breadcrumb-title.d-inline-block.mx-0= pageName
		
		.m-b-3
			ul.nav.nav-tabs-underline.justify-content-center(id="design-book-tabs").d-md-none
				li.nav-item
					a.nav-link.active(data-toggle="tab" href="#lucky-content") Your design(s)
				li.nav-item
					a.nav-link(data-toggle="tab" href="#wishlist-content") Collection

	.m-b-12.tab-content
		.tab-pane.fade(id="lucky-content").show.active
			.container-fluid
				h5.text-center.d-md-none-i.m-t-9.mb-0 Your design(s)
				.design-book-wrapper.p-t-8
					.row.px-2
						- for (var x = 1; x <= 9; x++)
								.col-sm-6.col-lg-4.px-4.py-3
									.item.is-gallery
										.decor-bg(style=`background-image: url(assets/images/gallery/gallery-h-${x}.jpg)`)
										.wrapper
											a(href="detail-album.html").item-list
												- for (var k = 1; k <= 4; k++)
													.item-thumbnail.custom-thumbnail
														.thumbnail
															.wrap-thumbnail-tag
																- if(k === 1)
																	img(src=`assets/images/gallery/gallery-v-${k}.jpg`)/
																- else if(k === 4 && x === 1)
																	img(src=`assets/images/gallery/dummy.jpg`)/
																- else
																	img(src=`assets/images/gallery/gallery-h-${k}.jpg`)/
											.item-content-overlay.d-lg-none-i
												.content-flex
													.item-left
														a(href="detail-album.html").title.d-block Morden Nail Salon
														a(href="#").category Nail Salon
													.item-right
														.btn-list.flex-center-y
															//- a(href="#" data-toggle="tooltip" title="34").btn.btn-just-icon.btn-white
															//- 	i.ri-heart-line
															.dropup.d-inline-flex
																.wrap-button(data-toggle="dropdown").m-r-3
																	button.btn.btn-just-icon.btn-white(data-toggle="tooltip" title="1,5k")
																		i.ri-share-line
																.dropdown-menu.dropdown-menu-right
																	a(href="#").dropdown-item.text-body
																		i.ri-facebook-box-fill
																		| Facebook
																	a(href="#").dropdown-item.text-body
																		i.ri-pinterest-fill
																		| Pinterest
																	a(href="#").dropdown-item.text-body
																		i.ri-mail-fill
																		| Mail
															a(href="detail-album.html#comment-container" data-toggle="tooltip" title="32").btn.btn-just-icon.btn-white
																i.ri-discuss-line
										//- .item-content-overlay.placement-top.d-lg-none-i
										//- 	.content-flex.justify-content-end
										//- 		.item-right
										//- 			.btn-list.flex-center-y
										//- 				a(href="assets/images/gallery/gallery-h-1.jpg" data-action="popup-zoom-in").btn.btn-just-icon.btn-white
										//- 					i.ri-fullscreen-line
										// Button Action
										.item-content-overlay.placement-bottom.d-lg-none(style="left: auto;")
											.content-flex.justify-content-end
												.item-right
													.dropup
														button(data-toggle="dropdown").btn.btn-just-icon.btn-white
															i.ri-more-line
														.dropdown-menu.dropdown-menu-right(style="min-width: 1px;").p-0
															//- a(href=imgFull  data-action="popup-zoom-in").text-body.dropdown-item.text-link.p-2.p-x-4
															//- 	i.ri-fullscreen-line.mr-0
															.js-action-mobile
																a(href="#").text-body.dropdown-item.text-link.p-2.p-x-4
																	i.ri-heart-fill.mr-0
																	span.badge.badge-brand.badge-notify 32
																.dropleft
																	button(data-toggle="dropdown").text-body.dropdown-item.text-link.p-2.p-x-4
																		i.ri-share-fill.mr-0
																		span.badge.badge-brand.badge-notify 1,2k
																	.dropdown-menu.dropdown-menu-right.p-0.m-0(style="height: 38px;min-width: 1px;").d-flex
																		a(href="#").dropdown-item.text-body.p-2
																			i.ri-facebook-box-fill
																		a(href="#").dropdown-item.text-body.p-2
																			i.ri-pinterest-fill
																		a(href="#").dropdown-item.text-body.p-2
																			i.ri-mail-fill
																a(href="detail-image.html#comment-container").text-body.dropdown-item.text-link.p-2.p-x-4
																	i.ri-discuss-line.mr-0
																	span.badge.badge-brand.badge-notify 106
									.d-block.text-center.mt-3
										a(href="detail-album.html").text-body.font-weight-semi Album #{x}

				.pagination
					span.sum.btn.btn-sm.btn-brand Page 1 of 10
					a(href="#").arrow.prev.btn.btn-sm Prev
					span.page.btn.btn-sm.btn-brand 1
					a(href="#").page.btn.btn-sm 2
					a(href="#").arrow.next.btn.btn-sm Next



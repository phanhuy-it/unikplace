		//- document.querySelectorAll('#wishlist-content .design-book-wrapper .item .item-content-overlay.d-lg-none a[data-action="popup-zoom-in"]').forEach((el, i)=> el.setAttribute('data-img-index', i));

		//- document.querySelectorAll('#lucky-content .design-book-wrapper .item .item-content-overlay.d-lg-none-i a[data-action="popup-zoom-in"]').forEach((el, i)=> el.setAttribute('data-img-index', i));
